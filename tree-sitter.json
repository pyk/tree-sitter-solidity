{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "solidity",
      "camelcase": "Solidity",
      "title": "Solidity",
      "scope": "source.solidity",
      "file-types": ["sol"],
      "injection-regex": "^solidity$",
      "class-name": "TreeSitterSolidity"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Solidity grammar for tree-sitter",
    "authors": [
      {
        "name": "pyk",
        "email": "gm@pyk.sh",
        "url": "https://pyk.sh/"
      }
    ],
    "links": {
      "repository": "http://github.com/pyk/tree-sitter-solidity"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
