{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "solidity",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_top_level_element"
      }
    },
    "_top_level_element": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "spdx_license_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "pragma_directive"
        }
      ]
    },
    "spdx_license_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\/\\/\\s*SPDX-License-Identifier:"
        },
        {
          "type": "FIELD",
          "name": "license",
          "content": {
            "type": "SYMBOL",
            "name": "license_identifier"
          }
        }
      ]
    },
    "license_identifier": {
      "type": "PATTERN",
      "value": "[\\w\\.-]+"
    },
    "pragma_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "pragma"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "identifier"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_pragma_token"
            }
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "_pragma_token": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "PATTERN",
          "value": "(\\d|\\.)+"
        },
        {
          "type": "PATTERN",
          "value": "[><=^~]+"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]\\w*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}